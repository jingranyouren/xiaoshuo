.user_info_wrap{
    height: 45vh;
    overflow: hidden;
    background-color: whitesmoke;
}
.user_info_wrap .user_img_wrap{
    position: relative;
}
.user_info_wrap .user_img_wrap .user_bg{
    height: 50vh;
    width: 100%;
    filter: blur(10rpx);
}
.user_info_wrap .user_img_wrap .user_info {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top:30%;
    text-align: center;
}
.user_info_wrap .user_img_wrap .user_info  .user_icon{
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}
.user_info_wrap .user_img_wrap .user_info .user_name{
    color: var(--themeColor);
    margin-top: 50rpx;
    font-size: 40rpx;
}
.user_info_wrap .login_btn{
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: 18%;
    border: 1rpx solid greenyellow;
    font-size: 38rpx;
}

.ul{
    padding-top: 10rpx;
    height: 100%;
}
.ul .li{
    background-color: var(--themeColor);
    margin-top: 10rpx;
    padding-top: 15rpx;
    display: flex;
    width: 100%;
    height: 90rpx;
    font-size: 40rpx;
    border: 1rpx solid darkgray;
}
.ul .li navigator{
    width: 100%;
    height: 100%;
    text-align:left;
    vertical-align:middle
}
.ul .li .text{
    text-align:right;
    height: 100%;
    color: darkgray;
    vertical-align:middle
}